{
  "name": "@supuwoerc/masonry",
  "version": "0.0.1",
  "module": "./dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./dist/": {
      "import": "./dist/"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite --host",
    "build": "rimraf build/**/* && tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts && copyfiles ./package.json build",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint:scripts": "eslint . --ext .ts",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix",
    "format": "pnpm run format:scripts && pnpm run format:styles",
    "prepare": "husky && echo 'npx lint-staged' > .husky/pre-commit && git add .husky/pre-commit",
    "uninstall-husky": "pnpm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky"
  },
  "lint-staged": {
    "src/**/*.{ts}": "pnpm run lint:scripts"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.38.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.10",
    "@vitest/coverage-v8": "^3.2.4",
    "copyfiles": "^2.4.1",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "stylelint": "^16.21.1",
    "stylelint-config-recommended": "^16.0.0",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "stylelint-order": "^7.0.0",
    "stylelint-prettier": "^5.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.0.3",
    "vitest": "^3.2.4",
    "globals": "^16.4.0",
    "release-it": "^19.0.5"
  }
}
