{
  "name": "@supuwoerc/masonry",
  "type": "module",
  "version": "0.0.1",
  "author": {
    "name": "Idris",
    "email": "supuwoerc@gmail.com",
    "url": "https://github.com/supuwoerc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supuwoerc/masonry"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/masonry.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "release": "pnpm test && pnpm build && pnpm release-it",
    "lint": "eslint . --fix",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "prepare": "husky"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@release-it/conventional-changelog": "^10.0.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.18.12",
    "@vitest/coverage-v8": "^3.2.4",
    "camelcase": "^8.0.0",
    "eslint": "^9.38.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.6",
    "release-it": "^19.0.6",
    "release-it-pnpm": "^4.6.6",
    "terser": "^5.44.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.10",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  }
}
